<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <managed-bean>
        <managed-bean-name>beanRegister</managed-bean-name>
        <managed-bean-class>com.daw.atlanthis.beans.register.beanRegister</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>beanLogin</managed-bean-name>
        <managed-bean-class>com.daw.atlanthis.beans.login.beanLogin</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <navigation-rule>
        <from-view-id>/inicio.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>true</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/register.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>true</from-outcome>
            <to-view-id>/inicio.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/templates/template.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/inicio.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>subcate</from-outcome>
            <to-view-id>/subcategoria.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/subcategoria.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>subcate</from-outcome>
            <to-view-id>/subcategoria.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
